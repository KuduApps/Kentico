}

.ImageEditorMain .MenuHeaderItemSelected
{
    border-bottom: 1px solid #999999 !important;
}

.ImageEditorMain .Divider
{
    background-color: #999999 !important;
}

.ImageEditorFrame
{
    border: 1px solid #999999 !important;
}


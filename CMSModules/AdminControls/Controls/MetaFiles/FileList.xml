<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions>
    <action name="edit" externalsourcename="edit" caption="$General.Edit$" icon="Edit.png" />
    <action name="delete" externalsourcename="delete" caption="$General.Delete$" icon="Delete.png" confirmation="$General.ConfirmDelete$" />
    <action name="paste" externalsourcename="paste" caption="$checkoutprocess.btnpastetooltip$" icon="Pasteimg.png" />
  </actions>
  <columns>
    <column source="##ALL##" externalsourcename="update" caption="$general.update$" wrap="false" style="text-align: center;" cssclass="UniGridActions" columnname="update" />
    <column source="##ALL##" externalsourcename="name" caption="$general.filename$" width="100%" wrap="false" sort="MetaFileName" />
    <column source="MetaFileSize" externalsourcename="size" caption="$general.size$" wrap="false" sort="MetaFileSize" />
    <column source="MetaFileName" caption="$general.filename$" wrap="false" columnname="filename">
      <filter type="text" />
    </column>
  </columns>
  <objecttype name="cms.metafile" />
  <options>
    <key name="DisplayFilter" value="true" />
  </options>
</grid>
